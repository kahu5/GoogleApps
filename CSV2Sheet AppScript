/**
 *  Imports a CSV hosted in Google Drive to the active sheet
 *  Add "Drive" under Services before clicking Run
 *  REPLACE "CSV1_ID" with the Google Drive file ID
 */

function importCSVFromGoogleDrive() {
  var getFileName = DriveApp.getFileById('CSV1_ID').getName();
console.log(getFileName)
  var file = DriveApp.getFilesByName(getFileName).next();
  var csvData = Utilities.parseCsv(file.getBlob().getDataAsString());
  var sheet = SpreadsheetApp.getActiveSheet();
    sheet.clearContents(); 
  sheet.getRange(1, 1, csvData.length, csvData[0].length).setValues(csvData);
}
